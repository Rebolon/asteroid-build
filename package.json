{
  "name": "asteroid-build",
  "version": "1.0.0",
  "description": "build of asteroid with babel-cli",
  "main": "index.js",
  "dependencies": {
    "asteroid": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babelify": "^7.3.0"
  },
  "peerDependencies": {
    "browserify": "^13.0.1",
    "minifyify": "^7.3.3"
  },
  "scripts": {
    "build": "browserify src/index.js -t babelify -d -p [minifyify --map app.js.map --output dist/app.js.map] --outfile dist/app.js"
  }
}
